{
  "name": "jsonapi-svelte-form",
  "type": "module",
  "main": "dist/build.js",
  "module": "dist/build.mjs",
  "unpkg": "dist/build.unpkg.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/build.mjs",
      "require": "./dist/build.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "test": "uvu src '\\.test\\.js$'",
    "build": "bundt src/index.js && rollup -c",
    "dev": "rollup -c -w",
    "serve": "sirv demo --dev --port 3000"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "bundt": "github:saibotsivad/bundt",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.43.0",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "dlv": "^1.1.3",
    "dset": "^3.1.0",
    "just-diff": "^3.1.1",
    "pointer-props": "^1.0.1"
  }
}
